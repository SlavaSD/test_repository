

def factorial(x):
  if x <= 1:
    return 1
  else:
    return x * factorial(x - 1)
  
  
  
def factorial_2(x):
  total = 1
  if x <= 1:
    return 1
  while x > 1:
    total *= x
    x -= 1
  return total


print factorial(-25)

print factorial_2(-25)


def factorial(x):
  total = 1
  while x > 0:
    total *= x
    x-=1
  return total
  
  
def factorial(x):
  while x > 0:
    a = x * factorial(x - 1)
    return a
  else:
    return 1
  
  
